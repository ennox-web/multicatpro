{
  "name": "multicatpro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "uvicorn src.api.index:app --reload --host 0.0.0.0 --port 8000",
    "dev": "docker compose up --remove-orphans --build",
    "k8s": "sh ./create-local-k8s-cluster.sh",
    "build": "next build",
    "start": "next start",
    "next-dev": "next dev",
    "lint": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "eslint-only": "next lint --ignore-path .gitignore",
    "cy:local": "cypress run --config-file cypress/config/dev.config.ts",
    "cy:staging": "cypress run --config-file cypress/config/staging.config.ts",
    "cy:prod": "cypress run --config-file cypress/config/prod.config.ts",
    "cy:open": "cypress open --config-file cypress/config/dev.config.ts",
    "db-api": "docker build -t ennox00/noxious:multicat-backend-v1 .\\src\\api\\.",
    "db-fe": "docker build -f Dockerfile.dev -t ennox00/noxious:multicat-frontend-v1 .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
